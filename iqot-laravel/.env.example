APP_NAME=IQOT
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# База данных (та же, что использует n8n)
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=iqot
DB_USERNAME=root
DB_PASSWORD=

# Для очередей (можно использовать database или redis)
QUEUE_CONNECTION=database

# Кэш
CACHE_STORE=database
SESSION_DRIVER=database

# n8n интеграция
N8N_WEBHOOK_URL=http://localhost:5678/webhook
N8N_API_KEY=your-n8n-api-key

# Telegram Bot (опционально)
TELEGRAM_BOT_TOKEN=

# Почта
MAIL_MAILER=smtp
MAIL_HOST=smtp.yandex.ru
MAIL_PORT=465
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=ssl
MAIL_FROM_ADDRESS="noreply@iqot.ai"
MAIL_FROM_NAME="${APP_NAME}"

# Filament Admin
FILAMENT_FILESYSTEM_DISK=public
